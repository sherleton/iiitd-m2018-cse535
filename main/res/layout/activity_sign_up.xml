<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/frontback"
    tools:context=".SignUpActivity"
    tools:layout_editor_absoluteY="25dp">

    <Button
        android:id="@+id/buttonlol2"
        android:layout_width="154dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="189dp"
        android:background="@android:color/holo_green_light"
        android:text="Sign Up"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="196dp"
        tools:layout_editor_absoluteY="424dp" />

    <EditText
        android:id="@+id/editrepass"
        android:layout_width="225dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/editname"
        android:layout_below="@+id/editpass"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="367dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="1dp"
        android:layout_height="1dp"
        app:srcCompat="@drawable/iiitd" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="6dp"
        android:layout_height="1dp"
        android:text="Phone No."
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="1dp"
        android:layout_height="1dp"
        app:srcCompat="@drawable/doctor" />

    <EditText
        android:id="@+id/editname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="59dp"
        android:layout_marginTop="214dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="66dp" />

    <EditText
        android:id="@+id/editroll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/editname"
        android:layout_marginTop="261dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="130dp" />

    <EditText
        android:id="@+id/editemail"
        android:layout_width="296dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/editname"
        android:layout_below="@+id/editphone"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="130dp" />

    <EditText
        android:id="@+id/editphone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/editname"
        android:layout_below="@+id/editroll"
        android:ems="10"
        android:inputType="phone"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="198dp" />

    <EditText
        android:id="@+id/editpass"
        android:layout_width="239dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/editname"
        android:layout_below="@+id/editemail"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="317dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="96dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/editpass"
        android:layout_alignParentStart="true"
        android:layout_marginStart="36dp"
        android:text="Password"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteY="320dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="127dp"
        android:layout_height="87dp"
        android:layout_alignBaseline="@+id/editrepass"
        android:layout_toStartOf="@+id/buttonlol2"
        android:paddingLeft="26dp"
        android:text="Re-enter your password"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteY="320dp" />

    <TextView
        android:id="@+id/Name"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/editname"
        android:layout_alignStart="@+id/textView12"
        android:text="Name"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="66dp" />

    <TextView
        android:id="@+id/name"
        android:layout_width="87dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/editroll"
        android:layout_alignStart="@+id/textView12"
        android:text="Roll no."
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="66dp" />

    <TextView
        android:id="@+id/text"
        android:layout_width="87dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/editphone"
        android:layout_alignStart="@+id/textView12"
        android:text="Phone no."
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="66dp" />

    <TextView
        android:id="@+id/textviewe"
        android:layout_width="87dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/editemail"
        android:layout_alignStart="@+id/textView12"
        android:text="Email ID"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="66dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="73dp"
        android:background="@android:color/background_dark"
        android:text="Already Have an Account ? Login Here !"
        android:textColor="@android:color/background_light" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:text="SIGNUP NOW TO AVAIL SERVICES FROM THE INFIRMARY @ IIIT - D !"
        android:textColor="@android:color/background_dark"
        android:textSize="24sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="144dp"
        android:layout_height="77dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="89dp"
        app:srcCompat="@drawable/iiitd" />

</RelativeLayout>